generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bank {
  id           Int      @id @default(autoincrement())
  nrInst       Int
  nrAgencia    Int
  cdClient     Int
  nmClient     String   @db.VarChar(100)
  nrCpfCnpj    String   @db.VarChar(50)
  nrContrato   Int
  dtContrato   DateTime
  qtPrestacoes Int
  vlTotal      String   @db.VarChar(15)
  cdProduto    Int
  dsProduto    String   @db.VarChar(100)
  cdCarteira   Int
  dsCarteira   String   @db.VarChar(100)
  nrProposta   Int
  nrPresta     Int
  tpPresta     String   @db.VarChar(50)
  nrSeqPre     Int
  dtVctPre     DateTime
  vlPresta     String   @db.VarChar(15)
  vlMora       String   @db.VarChar(15)
  vlMulta      String   @db.VarChar(15)
  vlOutAcr     String   @db.VarChar(15)
  vlIof        String   @db.VarChar(15)
  vlDescon     String   @db.VarChar(15)
  vlAtual      String   @db.VarChar(15)
  idSituac     String   @db.VarChar(100)
  idSitVen     String   @db.VarChar(100)

  @@map("dados_banco")
}
