generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bank {
  id            Int      @id @default(autoincrement())
  nr_inst       Int
  nr_agencia    Int
  cd_cliente    String   @db.VarChar(255)
  nm_cliente    String   @db.VarChar(100)
  nr_cpf_cnpj   String   @db.VarChar(50)
  nr_contrato   Int
  dt_contrato   DateTime
  qt_prestacoes Int
  vl_total      String   @db.VarChar(15)
  cd_produto    Int
  ds_produto    String   @db.VarChar(100)
  cd_carteira   Int
  ds_carteira   String   @db.VarChar(100)
  nr_proposta   Int
  nr_presta     Int
  tp_presta     String   @db.VarChar(50)
  nr_seq_pre    Int
  dt_vct_pre    DateTime
  vl_presta     String   @db.VarChar(15)
  vl_mora       String   @db.VarChar(15)
  vl_multa      String   @db.VarChar(15)
  vl_out_acr    String   @db.VarChar(15)
  vl_iof        String   @db.VarChar(15)
  vl_desconto   String   @db.VarChar(15)
  vl_atual      String   @db.VarChar(15)
  id_situac     String   @db.VarChar(100)
  id_sit_venc   String   @db.VarChar(100)

  @@map("dados_banco")
}
